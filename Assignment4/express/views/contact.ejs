
  <!-- CONTACT US  -->
    <main>
        <div class="box">
            <div class="inner-box">
                <div class="contact-info">
                    <h2>Contact Us</h2>
                    <div class="info">
                        <p><strong>Email:</strong> plants@gmail.com</p>
                        <p><strong>Phone:</strong> 123-456-7890</p>
                        <p><strong>Address:</strong> 123 ,street,country</p>
                    </div>
                    <img src="img/img1.png" alt="Company Photo">
                </div>

                <div class="forms-wrap">
                    <form action="#" autocomplete="off" class="contact-form">
                        <!-- <div class="heading">
                            <h2>Send us a message</h2>
                        </div> -->

                        <div class="actual-form">
                            <div class="input-wrap">
                                <label>Full Name</label>
                                <input type="text" class="input-field" autocomplete="off" required>
                               
                                <div class="error"></div> 
                            </div>

                            <div class="input-wrap">
                                <label>Email</label>
                                <input type="email" class="input-field" autocomplete="off" required>
                            
                                <div class="error"></div> 
                            </div>

                            <div class="input-wrap">
                                <label>Select Gender</label>
                                <select class="input-field" required>
                                    <option value="" disabled selected>Gender</option>
                                    <option value="male">Male</option>
                                    <option value="female">Female</option>
                                    <option value="other">Other</option>
                                </select>
                                <div class="error"></div> 
                            </div>

                            <div class="input-wrap">
                                <label>Message</label>
                                <textarea class="input-field" rows="4" required></textarea>
                            
                                <div class="error"></div> 
                            </div>

                            <input type="submit" value="Send Message" class="send-btn">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </main>
  
</body>
</html>